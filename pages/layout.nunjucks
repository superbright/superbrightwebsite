<!DOCTYPE HTML>
<!-- skel-baseline v3.0.0 | (c) n33 | skel.io | MIT licensed -->
<html>
	<head>
		<title>Superbright</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<script type="text/javascript" src="http://fast.fonts.net/jsapi/f9fa3103-6a5f-47f1-a0eb-e4889ee7b693.js"></script>
		<link rel="stylesheet" href="/css/main.css" />
		<link rel="stylesheet" href="/css/sb.css" />
	</head>
	<body id="top">

  <!-- You write code for this content block in another file -->
  {% block content %} {% endblock %}

		<!-- Footer -->
			<footer id="footer">
				<div class="container">
					<div class="row double">
						<div class="3u 12u$(medium)">

							<p>361 Stagg st #406<br>Brooklyn NY, 11206<br>hello@superbright.me</p>
							<ul class="icons">
											<li><a href="https://twitter.com/superbright_me" class="icon fa-twitter"><span class="label">Twitter</span></a></li>
											<li><a href="https://www.facebook.com/Superbrightme-685676934818450/" class="icon fa-facebook"><span class="label">Facebook</span></a></li>
											<li><a href="https://www.instagram.com/superbrightme/" class="icon fa-instagram"><span class="label">Instagram</span></a></li>
											<li><a href="https://github.com/superbright" class="icon fa-github"><span class="label">Github</span></a></li>

							</ul>
						</div>
						<div class="1u -8u 12u$(medium)" id="backtotop">
						Back to top
						</div>
					</div>
				</div>

			</footer>

		<!-- Scripts -->

			<script src="/js/jquery-2.1.4.min.js"></script>
			<script src="/js/queryloader2.min.js"></script>
			<script src="/js/skel.min.js"></script>
			<script src="/js/main.js"></script>
			<script src="/js/tagsystem.js"></script>

			<script type="text/javascript">
			    window.addEventListener('DOMContentLoaded', function() {
			        new QueryLoader2(document.querySelector("body"), {
			            barColor: "#69F7EB",
			            backgroundColor: "#fff",
			            percentage: false,
			            barHeight: 1250,
			            minimumTime: 1,
			            fadeOutTime: 1000
			        });
			  });

			    $( "#backtotop" ).click(function() {
				  jQuery("html,body").animate({scrollTop: 100}, 1000);
				});
</script>


	</body>
</html>
